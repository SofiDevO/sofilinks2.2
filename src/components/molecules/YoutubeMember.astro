---
import Support from "@data/support.json";
const {url, title} = Support.youtube;
---
<a href={url} target="_blank" class="w-full h-full flex flex-row items-center justify-center gap-3 p-2">
  <h3 class="text-text font-bold text-lg">{title}</h3>
  <iconify-icon icon="logos:youtube-icon" width="100" height="80"></iconify-icon>
</a>